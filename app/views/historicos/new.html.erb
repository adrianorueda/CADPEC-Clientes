<h1>Novo contato com <%= @cliente.nome %></h1>

<% form_for([@cliente, @historico]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label 'Observações' %><br />
    <%= f.text_area :observ %>
  </p>
  <p>
    <%= f.submit "Criar" %>
  </p>
<% end %>

<%= link_to 'Voltar', cliente_historicos_path(@cliente) %>